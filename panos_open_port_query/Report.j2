<h1 class="text-uppercase">Open Port Query Report</h1>
<h3>Query Parameters</h3>
<dl class="dl-horizontal">
    <dt>Firewall IP</dt>
    <dd>{{ TARGET_IP }}</dd>
    <dt>Source Zone</dt>
    <dd>'any' or '{{ untrust_zone_var }}'</dd>
    <dt>Policy Action</dt>
    <dd>'allow'</dd>
</dl>

<h3>Rulebase Matches Found</h3>
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Rule Name</th>
            <th>Application</th>
            <th>Service</th>
        </tr>
    </thead>
    <tbody>
        {% for rule  in full_output_as_object %}
        <tr>
            <td>{{ rule.entry["@name"] }}</td>
            <td>{{ rule.entry.application }}</td>
            <td>{{ rule.entry.service }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>

